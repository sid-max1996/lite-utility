{
  "name": "lite-utility",
  "version": "0.0.34",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "author": "sid-max1996",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sid-max1996/lite-utility.git"
  },
  "bugs": {
    "url": "https://github.com/sid-max1996/lite-utility/issues"
  },
  "homepage": "https://sid-max1996.github.io/lite-utility",
  "keywords": [
    "lite",
    "utility",
    "timer",
    "stopwatch",
    "slides",
    "emitter",
    "event",
    "state",
    "throttle",
    "debounce",
    "cache",
    "logger",
    "pipe functions",
    "try catch"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./services": {
      "import": "./services.js",
      "require": "./services.cjs"
    },
    "./logging": {
      "import": "./logging.js",
      "require": "./logging.cjs"
    },
    "./browser": {
      "import": "./browser.js",
      "require": "./browser.cjs"
    },
    "./react": {
      "import": "./react.js",
      "require": "./react.cjs"
    },
    "./vue": {
      "import": "./vue.js",
      "require": "./vue.cjs"
    }
  },
  "scripts": {
    "dev:docs": "npm run start --prefix docs-website",
    "build": "npm run check && vite build && npm run copy:pj && npm run copy:readme",
    "copy:pj": "node -e \"require('fs').cpSync('./package.json', './dist/package.json');\"",
    "copy:readme": "node -e \"require('fs').cpSync('./README.md', './dist/README.md');\"",
    "release": "npm run build && cd dist && npm publish",
    "preview": "vite preview",
    "check": "npm run prettier && npm run lint && tsc && vitest run",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.1",
    "react": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.2.0",
    "vue": "^3.4.18"
  }
}
